[
    {
        "selector": "node",
        "style": {
            "label": "data(name)",
            "width": 75,
            "height": 75
        }
    },
    {
        "selector": "edge",
        "style": {
            "width": 3,
            "opacity": 0.5,
            "line-color": "#888",
            "curve-style": "unbundled-bezier",
            "control-point-distances": 20,
            "control-point-weights": 0.5,
            "target-arrow-shape": "triangle",
            "label": "data(label)"
        }
    },
    {
        "selector": "[type = \"hidden\"]",
        "css": {
            "label": "",
            "border-width": 0,
            "background-opacity": 0
        }
    },
    {
        "selector": "[type = \"account\"]",
        "css": {
            "border-color": "#000000",
            "border-width": "1px",
            "border-style": "dashed",
            "background-opacity": 0
        }
    },
    {
        "selector": "[type = \"region\"]",
        "css": {
            "border-color": "#000000",
            "border-style": "solid",
            "background-color": "#c0c0f0",
            "background-opacity": 0.1
        }
    },
    {
        "selector": "[type = \"aws_vpc\"]",
        "css": {
            "border-color": "#6b788c",
            "border-style": "dotted",
            "background-color": "#f09900",
            "background-opacity": 0.1
        }
    },
    {
        "selector": "[type = \"az\"]",
        "css": {
            "border-color": "#f4ad42",
            "border-style": "dashed",
            "background-opacity": 0
        }
    },
    {
        "selector": "[type = \"aws_subnet\"]",
        "css": {
            "border-color": "#ff5959",
            "border-style": "dashed",
            "background-opacity": 0,
            "compound-sizing-wrt-labels": "include"
        }
    },
    {
        "selector": "[type = \"aws_instance\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Compute/Compute_AmazonEC2.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"grouped_ec2\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Compute/Compute_AmazonEC2_instances.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"autoscaling\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Compute/Compute_AmazonEC2_AutoScaling.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"aws_elb\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Compute/Compute_ElasticLoadBalancing.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"s3\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Storage/Storage_AmazonS3.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"rds\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Database/Database_AmazonRDS.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"rds_rr\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/Database/Database_AmazonRDS_instancereadreplica.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"ip\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/General/General_officebuilding.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    },
    {
        "selector": "[type = \"cloud\"]",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/aws/General/General_Internet.svg",
            "background-fit": "contain",
            "background-clip": "none",
            "width": 200,
            "height": 200
        }
    },
    {
        "selector": ":selected",
        "css": {
            "background-color": "#ff0",
            "background-opacity": 0.5
        }
    },
    {
        "selector": ".hiddenNeighbors",
        "css": {
            "border-width": 5,
            "border-color": "#000"
        }
    },
    {
        "selector": ".highlight",
        "css": {
            "width": 5,
            "opacity": 1,
            "line-color": "#888"
        }
    },
    {
        "selector": ".cy-expand-collapse-collapsed-node",
        "css": {
            "background-opacity": 0,
            "background-image": "${localSourceUri}/icons/cube.svg",
            "background-fit": "contain",
            "background-clip": "none"
        }
    }
]